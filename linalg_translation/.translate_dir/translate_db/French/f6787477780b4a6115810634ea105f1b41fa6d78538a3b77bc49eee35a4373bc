\begin{eg}
\begin{figure}[H]
    \centering
    \incfig{recouvrement-ouvert}
    \caption{recouvrement-ouvert}
    \label{fig:recouvrement-ouvert}
\end{figure}
\begin{itemize}
    \item $U_x = B(x, \frac{1}{2})$
    \item $\bigcup_{x \in F} U_x$ contient $F$
    \item  $(U_x)_{x \in F}$ recouvrement ouvert de $F$
\end{itemize}
\end{eg}